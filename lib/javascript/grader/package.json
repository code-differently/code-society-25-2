    "devDependencies": {
        "@google/clasp": "^2.4.2",
        "@types/google-apps-script": "^1.0.83",
        "typescript": "^5.3.3"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "keywords": [
        "google-apps-script",
        "typescript",
        "clasp",
        "grader"
    ],
