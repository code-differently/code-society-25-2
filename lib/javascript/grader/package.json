{
    "name": "grader-apps-script",
    "version": "1.0.0",
    "description": "Google Apps Script project for grading functionality",
    "main": "index.js",
    "scripts": {
        "type-check": "tsc --noEmit",
        "watch": "clasp push --watch",
        "push": "clasp push",
