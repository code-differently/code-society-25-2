    },
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "python-envs.pythonProjects": [
        {
            "path": "lesson_22/trinitiejackson",
            "envManager": "ms-python.python:venv",
